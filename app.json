{
  "expo": {
    "name": "Bloom",
    "slug": "bloom-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bloom.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bloom.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "build": {
        "babel": {
          "include": [
            "@tamagui/core"
          ]
        }
      }
    },
    "sdkVersion": "52.0.0",
    "plugins": [],
    "extra": {
      "expoGo": true
    },
    "owner": "franciswade"
  }
}
